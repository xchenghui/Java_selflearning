package com.x20180703;

public class Demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

class Cat extends Thread
{
	public void run()
	{
		System.out.println("11");
	}
}


class Dog implements Runnable
{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println("22");
	}
	
}











