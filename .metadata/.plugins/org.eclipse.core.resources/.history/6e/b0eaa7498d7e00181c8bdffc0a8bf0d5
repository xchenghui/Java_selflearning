/*
 * 如何通过继承Thread来开发线程
 */


package com.x20180702;

public class Demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		//创建一个Cat、对象
		Cat cat = new Cat();
		
		//启动线程
		cat.start();
	}

}

class Cat extends Thread
{
	//重新编写run函数
	public void run()
	{
		System.out.println("Hello, World");
	}
}











